parameters:
  # Paramentri DB Default
  db_driver: {{ db_driver }}
  db_host: {{ db_host }}
  db_port: {{ db_port }}
  db_name: {{ db_name }}
  db_user: {{ db_user }}
  db_password: {{ db_password }}

  # Altri paramentri
  mailer_transport:  {{ mailer_transport }}
  mailer_host:       {{ mailer_host }}
  mailer_user:       {{ mailer_user }}
  mailer_password:   {{ mailer_password }}

  locale:            {{ locale }}
  secret:            {{ secret }}

  debug_toolbar:          {{ debug_toolbar }}
  debug_redirects:        {{ debug_redirects }}
  use_assetic_controller: {{ use_assetic_controller }}
